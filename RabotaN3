        case 4:
        cout << "Апрель";
        break;
    
        case 5:
        cout << "Май";
        break;
    
        case 6:
        cout << "Июнь";
        break;
    
        case 7:
        cout << "Июль";
        break;
    
        case 8:
        cout << "Август";
        break;
    
        case 9:
        cout << "Сентябрь";
        break;
    
        case 10:
        cout << "Октябрь";
        break;
    
        case 11:
        cout << "Ноябрь";
        break;
    
        case 12:
        cout << "Декабрь";
        break;
        default:
        cout << " Good ";
